{
    "name": "webhook-listener-api",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "test": "sst bind vitest run",
        "test:webhooks": "sst bind vitest packages/functions/__tests__/api/webhooks.test.ts",
        "dev": "sst dev",
        "build": "sst build",
        "deploy": "sst deploy",
        "remove": "sst remove",
        "console": "sst console",
        "typecheck": "tsc --noEmit",
        "typegen": "sst types",
        "typegen:db": "sst bind tsx ./packages/core/src/utils/db-type-generator.ts generate",
        "sso": "aws sso login --sso-session=test",
        "sso:clean": "rm -rf ~/.aws/sso/cache/*",
        "migrate": "sst bind tsx ./packages/core/src/utils/db-migrator.ts",
        "migrate:help": "sst bind tsx ./packages/core/src/utils/db-migrator.ts --help",
        "migrate:list": "sst bind tsx ./packages/core/src/utils/db-migrator.ts list",
        "script:create": "sst bind tsx ./packages/functions/scripts/create.ts dev",
        "script:delete": "sst bind tsx ./packages/functions/scripts/delete.ts dev",
        "script:update": "sst bind tsx ./packages/functions/scripts/update.ts dev",
        "trigger:cron": "sst bind tsx ./packages/functions/src/appt-reminder/cron.ts dev",
        "trigger:reminders": "sst bind tsx ./packages/functions/src/appt-reminder/queue/main.ts dev",
        "script:dev": "sst bind tsx ./packages/functions/src/dev.ts dev",
        "render-template:reminder": "ejs ./packages/core/src/templates/appt-reminder/template.ejs -f ./packages/core/src/templates/appt-reminder/data.json -o ./DELETE_ME_render.html"
    },
    "devDependencies": {
        "@tsconfig/node18": "^18.2.2",
        "@types/ejs": "^3.1.5",
        "aws-cdk-lib": "2.171.1",
        "constructs": "10.3.0",
        "sst": "^2.39.6",
        "tsx": "^4.17.0",
        "typescript": "^5.3.3",
        "vitest": "^1.6.0",
        "kysely-codegen": "^0.10.0"
    },
    "workspaces": [
        "packages/*"
    ],
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.525.0",
        "@aws-sdk/client-rds-data": "^3.620.0",
        "@aws-sdk/client-ses": "^3.504.0",
        "@aws-sdk/client-sns": "^3.651.1",
        "@aws-sdk/client-sqs": "^3.515.0",
        "@paralleldrive/cuid2": "^2.2.2",
        "date-fns": "^4.1.0",
        "date-fns-tz": "^3.1.3",
        "ejs": "^3.1.9",
        "fast-fuzzy": "^1.12.0",
        "google-auth-library": "^9.11.0",
        "google-spreadsheet": "^4.1.2",
        "jsonschema": "^1.4.1",
        "kysely": "^0.27.4",
        "kysely-data-api": "^1.0.1",
        "node-fetch": "^3.3.2",
        "snowflake-sdk": "^1.11.0",
        "tweetnacl": "^1.0.3",
        "tweetnacl-util": "^0.15.1"
    }
}